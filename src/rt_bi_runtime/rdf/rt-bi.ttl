@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://rezateshnizi.com/rt-bi/> a owl:Ontology ;
	dc:title "The vocabulary used in RT Behavior Inference." ;
	dc:date "2024-02-08" ;
	dc:description "This is the RDF Schema for the terms in RT Behavior Automata, defined in the dissertation of Reza Teshnizi." .

@prefix rt_bi: <https://rezateshnizi.com/rt-bi/> .
# region Class definitions
rt_bi:X a rdf:Class .
rt_bi:Y a rdf:Class .
rt_bi:Vertex a rdf:Class .
rt_bi:VertexSeq a rdf:Seq .
rt_bi:Polygon a rdf:Class .

rt_bi:Time a rdf:Class .
rt_bi:Interval a rdf:Class .

rt_bi:Subject a rdf:Class .
rt_bi:RegularSpace a rdf:Class ;
	rdfs:subClassOf rt_bi:Subject ;
	rdfs:comment "The class of RegularSpace specifications." .
rt_bi:RegularTime a rdf:Class ;
	rdfs:subClassOf rt_bi:Subject ;
	rdfs:comment "The class of RegularTime specifications." .
rt_bi:Traversability a rdf:Class ;
	rdfs:comment "The class of traversability specifications." .

# endregion Class definitions
@prefix temporal_props: <https://rezateshnizi.com/rt-bi/temporal/props#> .
# region temporal
temporal_props:secs a rdf:value ;
	rdfs:domain rt_bi:Time ;
	rdfs:range xsd:double ;
	rdfs:comment "A time value in Seconds." .
temporal_props:min a rdf:Property ;
	rdfs:domain rt_bi:Interval ;
	rdfs:range rt_bi:Time ;
	rdfs:comment "The min bound of a time interval." .
temporal_props:max a rdf:Property ;
	rdfs:domain rt_bi:Interval ;
	rdfs:range rt_bi:Time ;
	rdfs:comment "The max bound of a time interval." .
temporal_props:include_min a rdf:Property ;
	rdfs:domain rt_bi:Interval ;
	rdfs:range xsd:boolean ;
	rdfs:comment "Whether the min bound is included in the interval" .
temporal_props:include_max a rdf:Property ;
	rdfs:domain rt_bi:Interval ;
	rdfs:range xsd:boolean ;
	rdfs:comment "Whether the max bound is included in the interval" .
# endregion temporal
@prefix spatial_props: <https://rezateshnizi.com/rt-bi/spatial/props#> .
# region spatial
spatial_props:vertices a rdf:Property ;
	rdfs:domain rt_bi:Polygon ;
	rdfs:range rt_bi:VertexSet ;
	rdfs:comment "The vertices of a polygon." .
spatial_props:x a rdf:Property ;
	rdfs:domain rt_bi:Vertex ;
	rdfs:range rt_bi:X ;
	rdfs:comment "The x Coordinate." .
spatial_props:y a rdf:Property ;
	rdfs:domain rt_bi:Vertex ;
	rdfs:range rt_bi:Y ;
	rdfs:comment "The y Coordinate." .
spatial_props:x_val a rdf:value ;
	rdfs:domain rt_bi:X ;
	rdfs:range xsd:double ;
	rdfs:comment "Some x value." .
spatial_props:y_val a rdf:value ;
	rdfs:domain rt_bi:Y ;
	rdfs:range xsd:double ;
	rdfs:comment "Some y value." .
# endregion spatial
@prefix world_props: <https://rezateshnizi.com/rt-bi/world/props#> .
# region world
world_props:name a rdfs:label ;
	rdfs:domain rt_bi:Subject ;
	rdfs:range xsd:string ;
	rdfs:comment "The name of this subject." .
world_props:color a rdf:Property ;
	rdfs:domain rt_bi:Subject ;
	rdfs:range xsd:string ;
	rdfs:comment "Color to describe this area (and use in render)." .
world_props:polygons a rdf:Property ;
	rdfs:domain rt_bi:RegularSpace ;
	rdfs:range rt_bi:Polygon .
world_props:intervals a rdf:Property ;
	rdfs:domain rt_bi:RegularTime ;
	rdfs:range rt_bi:Interval .
world_props:off_times a rdf:Property ;
	rdfs:domain rt_bi:RegularSpace ;
	rdfs:range rt_bi:Interval .
world_props:traversability a rdf:Property ;
	rdfs:domain rt_bi:RegularSpace ;
	rdfs:range rt_bi:Traversability ;
	rdfs:comment "The range of this property is a traversability specification." .
world_props:legs a rdf:Property ;
	rdfs:domain rt_bi:Traversability ;
	rdfs:range xsd:boolean ;
	rdfs:comment "Whether the agent moves on legs." .
world_props:wheels a rdf:Property ;
	rdfs:domain rt_bi:Traversability ;
	rdfs:range xsd:boolean ;
	rdfs:comment "Whether the agent moves on wheels." .
world_props:swims a rdf:Property ;
	rdfs:domain rt_bi:Traversability ;
	rdfs:range xsd:boolean ;
	rdfs:comment "Whether the agent moves through water." .
# endregion world
