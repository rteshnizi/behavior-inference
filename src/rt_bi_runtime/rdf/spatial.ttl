						]
						[	spatial:vertex_ind 6;
							spatial:vertex_coords pt:9;
						]
						[	spatial:vertex_ind 7;
							spatial:vertex_coords pt:1;
						]
					) .
polygon:1 rdf:value (
						[	spatial:vertex_ind 0;
							spatial:vertex_coords pt:1;
