@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix rt_bi: <https://rezateshnizi.com/rt-bi-ontology/> .

<https://rezateshnizi.com/rt-bi-ontology/> a owl:Ontology ;
	dc:title "The vocabulary used in RT Behavior Inference." ;
	dc:date "2024-01-31" ;
	dc:description "This is the RDF Schema for the terms in RT Behavior Automata, defined in the dissertation of Reza Teshnizi." .

############## ABSTRACT ##############
rt_bi:object a rdfs:Resource ;
	rdfs:label "RT-object" ;
	rdfs:comment "An instance of a class." .

rt_bi:Property a rdf:Property ;
	rdfs:label "RT-Property" ;
	rdfs:comment "Properties of objects." ;
	rdfs:domain rt_bi:object .

############## THE OBJECTS ##############
rt_bi:Agent a rt_bi:object ;
	rdfs:label "Agent" ;
	rdfs:comment "A term describing an agent." .

rt_bi:Space a rt_bi:object ;
	rdfs:label "Space" ;
	rdfs:comment "A term describing a spatial region." .

rt_bi:Time a rt_bi:object ;
	rdfs:label "Time" ;
	rdfs:comment "A term describing a temporal region." .

############## THE PROPERTIES ##############
rt_bi:movement a rt_bi:Property ;
	rdfs:label "movement" ;
	rdfs:comment "Movement properties of an object." ;
	rdfs:domain rt_bi:object ;
	rdfs:range rt_bi:MovementType .

rt_bi:MovementType a rdfs:Resource ;
	rdfs:label "MovementType" ;
	rdfs:comment "The various ways of moving in a given environment." ;
	rdfs:subClassOf rt_bi:Property ;
	rdfs:domain rt_bi:movement .

rt_bi:walks a rt_bi:MovementType ;
	rdfs:label "walks" ;
	rdfs:comment "Whether subject can walk." ;
	rdfs:domain rt_bi:movement ;
	rdfs:range xsd:boolean .

rt_bi:drives a rt_bi:MovementType ;
	rdfs:label "drives" ;
	rdfs:comment "Whether subject can drive." ;
	rdfs:domain rt_bi:movement ;
	rdfs:range xsd:boolean .

rt_bi:swims a rt_bi:MovementType ;
	rdfs:label "swims" ;
	rdfs:comment "Whether subject can swim." ;
	rdfs:domain rt_bi:movement ;
	rdfs:range xsd:boolean .

rt_bi:color a rt_bi:Property ;
	rdfs:label "color" ;
	rdfs:comment "The visible color of the object." ;
	rdfs:domain rt_bi:object ;
	rdfs:range rdfs:Literal .

rt_bi:business_hours a rt_bi:Property ;
	rdfs:label "business_hours" ;
	rdfs:comment "Indicator of whether this is a business hour time." ;
	rdfs:domain rt_bi:object ;
	rdfs:range xsd:boolean .
