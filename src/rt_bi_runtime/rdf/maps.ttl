@base <https://rezateshnizi.com/rt-bi-ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix region_type: <regions/types#> .
@prefix polygon: <spatial/polygons#> .
@prefix time_interval: <times/time-intervals#> .

############## DEFINITIONS ##############
@prefix map: <maps/> .
map:polygons a rdf:predicate .
map:off_times a rdf:predicate .
map:region_types a rdf:predicate .

@prefix map_part: <maps/parts#> .
map_part:0 map:polygons (polygon:0) ;
	map:region_types (region_type:1) ;
	map:off_times () .
map_part:1 map:polygons (polygon:1) ;
	map:region_types (region_type:2) ;
	map:off_times () .
map_part:2 map:polygons (polygon:2) ;
	map:region_types (region_type:1) ;
	map:off_times () .
map_part:3 map:polygons (polygon:3) ;
	map:region_types (region_type:0) ;
	map:off_times () .
map_part:4 map:polygons (polygon:4) ;
	map:region_types (region_type:1) ;
	map:off_times () .
map_part:5 map:polygons (polygon:5) ;
	map:region_types (region_type:0) ;
	map:off_times (time_interval:0 time_interval:3) .
map_part:6 map:polygons (polygon:5) ;
	map:region_types (region_type:2) ;
	map:off_times (time_interval:1 time_interval:2 time_interval:4) .
map_part:7 map:polygons (polygon:6) ;
	map:region_types (region_type:2) ;
	map:off_times () .
