@base <https://rezateshnizi.com/rt-bi-ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .

############## DEFINITIONS ##############
@prefix agents: <agents/> .
agents:legs a rdf:predicate ;
	rdfs:range xml:boolean ;
	rdfs:comment "Whether the agent moves on legs." .
agents:wheels a rdf:predicate ;
	rdfs:range xml:boolean ;
	rdfs:comment "Whether the agent moves on wheels." .
agents:swims a rdf:predicate ;
	rdfs:range xml:boolean ;
	rdfs:comment "Whether the agent moves through water." .

@prefix agent_type: <agents/types#> .
agent_type:0 rdfs:label "pedestrian" ;
	agents:legs true ;
	agents:wheels false ;
	agents:swims false .
agent_type:1 rdfs:label "vehicle" ;
	agents:legs false ;
	agents:wheels true ;
	agents:swims false .
agent_type:2 rdfs:label "boat" ;
	agents:legs false ;
	agents:wheels false ;
	agents:swims true .
